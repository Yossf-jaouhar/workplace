package main

import (
	"fmt"
	"os"
	"strconv"
)

func main(){
	if len(os.Args) != 2 {
		return
	}

	str := os.Args[1]
	for _, char := range str {
		if char < '0' || char > '9' {
			return
		}
	}
	v := len(str) != 0
	nm , _ := strconv.Atoi(str)

	if nm < 2 {
		return
	}
	slice := []int{2,3,5,7,9,11,13,17,23}

	res := ""
	for i:=0; i < len(slice) ; i++ {
		for {
			if nm % slice[i] == 0 {
				res += strconv.Itoa(slice[i]) + "*"
				nm /= slice[i]
				continue
			} else {
				break
			}
		}
	}

	if res != "" {
		res = res[:len(res)-1]
		fmt.Println(res)
		return
	}

	if v {
		fmt.Println(str)
	} else {
		return
	}
}